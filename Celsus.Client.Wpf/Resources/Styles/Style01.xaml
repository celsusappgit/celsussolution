    <Style x:Key="RadBusyIndicator" TargetType="telerik:RadBusyIndicator">
        <Setter Property="BusyContent" Value="{x:Null}"></Setter>
        <Setter Property="Background" Value="Transparent"></Setter>
        <Setter Property="Content" Value="{x:Null}"></Setter>
        <Setter Property="IsIndeterminate" Value="True"></Setter>
        <Setter Property="VerticalAlignment" Value="Stretch"></Setter>
        <Setter Property="HorizontalAlignment" Value="Stretch"></Setter>
        <Setter Property="OverlayStyle">
            <Setter.Value>
                <Style TargetType="Rectangle">
                    <Setter Property="Stroke" Value="Transparent" />
                    <Setter Property="Fill">
                        <Setter.Value>
                            <RadialGradientBrush 
                                     MappingMode="RelativeToBoundingBox">
                                <GradientStop Color="#336A6A6A" Offset="0" />
                                <GradientStop Color="#996A6A6A" Offset="0.5" />
                            </RadialGradientBrush>
                        </Setter.Value>
                    </Setter>

                    <Setter Property="Opacity" Value="0.2" />
                </Style>
            </Setter.Value>
        </Setter>
    </Style>

    
